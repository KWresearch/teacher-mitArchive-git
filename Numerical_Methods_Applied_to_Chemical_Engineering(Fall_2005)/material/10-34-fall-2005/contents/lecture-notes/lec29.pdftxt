Rxn.ratelaw_exp(irxn,:) = [1 2 0 0];

Rxn.k_ref(irxn) = 1;

Rxn.T_ref(irxn) = 1;

Rxn.E_activ(irxn) = 1;

Rxn.delta_H(irxn) = -1;


% Set reaction # 2 data 
